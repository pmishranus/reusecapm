{
    "name": "employee-master",
    "version": "1.0.0",
    "description": "Employee Master Data CAPM Application - Standalone Service",
    "main": "index.js",
    "scripts": {
        "start": "cds run",
        "build": "cds build",
        "deploy": "cds deploy",
        "watch": "cds watch",
        "test": "npm run test:unit",
        "test:unit": "jest",
        "test:integration": "jest --testPathPattern=integration"
    },
    "dependencies": {
        "@sap/cds": "^7.0.0",
        "express": "^4.18.0",
        "passport": "^0.6.0",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@sap/cds-dk": "^7.0.0",
        "sqlite3": "^5.1.0",
        "jest": "^29.0.0"
    },
    "cds": {
        "requires": {
            "db": {
                "kind": "hana"
            },
            "auth": {
                "kind": "xsuaa"
            }
        },
        "provides": {
            "EmployeeService": {
                "kind": "odata",
                "model": "srv/employee-service"
            }
        }
    },
    "engines": {
        "node": ">=16.0.0"
    }
}