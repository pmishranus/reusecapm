{
    "xsappname": "employee-master",
    "tenant-mode": "dedicated",
    "scopes": [
        {
            "name": "$XSAPPNAME.Admin",
            "description": "Admin"
        },
        {
            "name": "$XSAPPNAME.User",
            "description": "User"
        }
    ],
    "role-templates": [
        {
            "name": "Admin",
            "scope-references": [
                "$XSAPPNAME.Admin"
            ]
        },
        {
            "name": "User",
            "scope-references": [
                "$XSAPPNAME.User"
            ]
        }
    ],
    "oauth2-configuration": {
        "redirect-uris": [
            "https://*.cfapps.sap.hana.ondemand.com/**"
        ]
    }
}